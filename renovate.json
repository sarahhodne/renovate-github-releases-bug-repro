{
  "regexManagers": [
    {
      "fileMatch": ["repro"],
      "matchStrings": [
        "GORELEASER_VERSION=\"(?<currentValue>.*?)\"\\s+GORELEASER_CHECKSUM=\"(?<currentDigest>.*?)\"",
      ],
      "depNameTemplate": "goreleaser/goreleaser",
      "datasourceTemplate": "github-releases"
    }
  ]
}
